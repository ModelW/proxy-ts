{
  "name": "@model-w/proxy",
  "version": "0.2.1",
  "description": "Event Handler Proxy to make the redirections from Vue3 templates to the ServerTemplatedComponent render",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModelW/proxy-ts.git"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/ModelW/proxy-ts/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "homepage": "https://github.com/ModelW/proxy-ts#readme",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint nuxt-module",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1",
    "h3": "^1.6.4",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.1",
    "@nuxt/test-utils": "^3.4.1",
    "changelogen": "^0.5.3",
    "eslint": "^8.38.0",
    "nuxt": "^3.4.1",
    "vitest": "^0.30.1"
  },
  "contributors": [
    "Laurent Treguier",
    "Ivan Lorenzo"
  ]
